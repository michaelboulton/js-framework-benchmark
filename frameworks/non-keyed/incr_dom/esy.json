{
  "name": "js-framework-benchmark-incr_dom",
  "version": "1.0.0",
  "description": "Incr_dom version of benchmark",
  "author": "Michael Boulton",
  "license": "Apache-2.0",
  "homepage": "https://github.com/krausest/js-framework-benchmark",
  "repository": {
    "type": "git",
    "url": "https://github.com/krausest/js-framework-benchmark.git"
  },
  "keywords": [
    "webpack",
    "ReasonML",
    "incr_dom"
  ],
  "esy": {
    "build": "dune build --root . -j 3 --verbose --profile release",
    "install": [
      "esy-installer js-framework-benchmark.install"
    ],
    "release": {
      "releasedBinaries": []
    }
  },
  "scripts": {
    "run": "esy x bash -c 'http-server #{self.bin}'",
    "build": "esy b",
    "build-prod": "esy b dune build --root . -j 3 --verbose --profile release"
  },
  "peerDependencies": {
    "ocaml": "^4.7.0"
  },
  "dependencies": {
    "@opam/incr_dom": "v0.12.0",
    "@opam/incr_dom_partial_render": "v0.12.0",
    "@reason-native/console": "*"
  },
  "devDependencies": {
    "http-server": "*",
    "@esy-ocaml/reason": "3.4.0",
    "@opam/merlin": "3.2.2",
    "refmterr": "3.1.10"
  }
}
